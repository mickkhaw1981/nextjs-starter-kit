{
  "name": "nextjs-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "use:local": "node scripts/switchEnv.js local && prisma generate",
    "use:staging": "node scripts/switchEnv.js staging && prisma generate",
    "use:prod": "node scripts/switchEnv.js production && prisma generate",
    "db:current": "node -e \"const env = require('dotenv').config().parsed; console.log('\\nðŸ“¡ Current Environment:\\n', 'DATABASE_URL:', env?.DATABASE_URL || 'Not set', '\\nDIRECT_URL:', env?.DIRECT_URL || 'Not set', '\\nEnvironment file:', require('fs').existsSync('.env') ? '.env exists' : '.env not found', '\\n')\"",
    "db:check": "prisma format && prisma validate"
  },
  "prisma": {
    "seed": "tsx prisma/seeds/seed.ts"
  },
  "packageManager": "npm@11.1.0",
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.482.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^8.5.5",
    "@storybook/addon-essentials": "^8.5.5",
    "@storybook/addon-interactions": "^8.5.5",
    "@storybook/addon-onboarding": "^8.5.5",
    "@storybook/blocks": "^8.5.5",
    "@storybook/nextjs": "^8.5.5",
    "@storybook/react": "^8.5.5",
    "@storybook/test": "^8.5.5",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^6.5.0",
    "storybook": "^8.5.5",
    "supabase": "^2.20.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.4.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
